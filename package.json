{
    "name": "gridmath",
    "version": "1.0.0",
    "description": "Table board for math calculations",
    "main": "electron/main.cjs",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack-dev-server --open",
        "build:web": "webpack --mode production",
        "build": "npm run build:web",
        "dist:mac": "npm run build:web && electron-builder --mac dmg --publish never",
        "dist:win": "npm run build:web && electron-builder --win nsis --publish never",
        "dist:linux": "npm run build:web && electron-builder --linux AppImage deb --publish never"
    },
    "keywords": [
        "react",
        "typescript",
        "scss",
        "webpack",
        "babel"
    ],
    "moduleFileExtensions": [
        "js",
        "ts",
        "tsx"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.22.17",
        "@babel/preset-env": "^7.22.15",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.22.15",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.8.1",
        "electron": "^31.7.7",
        "electron-builder": "^24.13.3",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.3",
        "sass": "^1.66.1",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.4",
        "typescript": "^5.2.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "@reduxjs/toolkit": "^1.9.7",
        "@types/react-redux": "^7.1.27",
        "bootstrap": "^5.3.2",
        "classnames": "^2.3.2",
        "i": "^0.3.7",
        "npm": "^10.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.3",
        "redux": "^4.2.1"
    },
    "build": {
        "appId": "su.linka.gridmath",
        "productName": "GridMath",
        "directories": {
            "output": "release"
        },
        "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
        "files": [
            "build/**/*",
            "electron/**/*",
            "package.json"
        ],
        "mac": {
            "icon": "assets-icons/icon.icns",
            "hardenedRuntime": true,
            "entitlements": "electron/entitlements.mac.plist",
            "entitlementsInherit": "electron/entitlements.mac.plist",
            "gatekeeperAssess": false,
            "target": [
                "dmg"
            ]
        },
        "win": {
            "icon": "assets-icons/icon.ico",
            "target": [
                "nsis"
            ]
        },
        "linux": {
            "icon": "assets-icons/icon.png",
            "target": [
                "AppImage",
                "deb"
            ]
        }
    }
}
